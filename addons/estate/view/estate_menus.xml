<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record id="test_model_action" model="ir.actions.act_window">
    <field name="name">properties</field>
    <field name="res_model">estate.property</field>
    <field name="view_mode">tree,form</field>


    </record>

     <record id="test_model_action_type" model="ir.actions.act_window">
    <field name="name">Properties Type</field>
    <field name="res_model">estate.property.type</field>
    <field name="view_mode">tree,form</field>
    </record>

    <record id="test_model_action_tag" model="ir.actions.act_window">
    <field name="name">Properties Tag</field>
    <field name="res_model">estate.property.tag</field>
    <field name="view_mode">tree,form</field>
    </record>




<menuitem id="test_menu_root" name="Real estate" >

    <menuitem id="test_first_level_menu" name="Advertisements">
        <menuitem id="test_model_menu_property" action="test_model_action"/>
    </menuitem>

     <menuitem id="test_second_level_menu" name="Settings">
        <menuitem id="test_model_menu_type" action="test_model_action_type"/>
         <menuitem id="test_model_menu_tag" action="test_model_action_tag"/>
    </menuitem>





</menuitem>





</odoo>